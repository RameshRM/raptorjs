{
    "type": "raptor-module",
    "name": "json",
    "version": "1.0",
    "description": "Raptor Module: widgets",
    "homepage": "http://wiki2.arch.ebay.com/display/RAPTOR/Home",
    "authors": [
        {
            "name": "Patrick Steele-Idem",
            "email": "psteeleidem@ebay.com"
        }
    ],
    
    "raptor": {
        "dependencies": [
            {"path": "json.js"}
        ],
    
        "extensions": {
            "jquery": {
                "dependencies": [
                    {"path": "json_jquery.js"}
                ]
            },
            "json.raptor": {
                "dependencies": [
                    {"module": "json.parse"},
                    {"module": "json.stringify"},
                    {"path": "jsonParse_raptor.js"},
                    {"path": "jsonStringify_raptor.js"}
                ]
            },
            "json.raptorStringify": {
                "dependencies": [
                    {"module": "json.stringify"},
                    {"path": "jsonStringify_raptor.js"}
                ]
            },
            "json.raptorParse": {
                "dependencies": [
                    {"module": "json.parse"},
                    {"path": "jsonParse_raptor.js"}
                ]
            }
        }
    }
}
