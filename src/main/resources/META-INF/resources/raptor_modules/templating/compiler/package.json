{
    "type": "raptor-module",
    "name": "templating.compiler",
    "version": "1.0",
    "description": "RaptorJS JavaScript Template Compiler",
    "homepage": "http://wiki2.arch.ebay.com/display/RAPTOR/Home",
    "authors": [
        {
            "name": "Patrick Steele-Idem",
            "email": "psteeleidem@ebay.com"
        }
    ],
    
    "raptor": {
        "includes": [
            {"module": "resources"},
            {"module": "regexp"},
            {"module": "strings"},
            {"module": "objects"},
            {"module": "listeners"},
            {"module": "json.stringify"},
            {"module": "xml.utils"},
            {"module": "xml.sax.object-mapper"},
            {"path": "Expression.js"},
            {"path": "ExpressionParser.js"},
            {"path": "TypeConverter.js"},
            {"path": "AttributeSplitter.js"},
            {"path": "TemplateBuilder.js"},
            {"path": "TemplateCompiler.js"},
            {"path": "Taglib.js"},
            {"path": "TaglibCollection.js"},        
            {"path": "compiler.js"},
            {"path": "Node.js"},
            {"path": "ElementNode.js"},
            {"path": "TextNode.js"},
            {"path": "ParseTreeBuilder.js"},
            {"path": "TaglibXmlLoader.js"},
            {"path": "EscapeXmlContext.js"}
        ],
        
        "extensions": [
            {   
                "name": "server",
                "includes": [
                    {"path": "compiler_server.js"}
                ]
            },
            {   
                "name": "rhino",
                "includes": [
                    {"path": "compiler_rhino.js"}
                ]
            },
            {   
                "name": "browser",
                "includes": [
                    {"path": "compiler_browser.js"}
                ]
            }
        ]
    }
}
