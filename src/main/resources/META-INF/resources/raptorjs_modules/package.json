{
    "type": "library",
    "name": "raptorjs",
    "version": "1.0",
    "description": "RaptorJS",
    "homepage": "http://wiki2.arch.ebay.com/display/RAPTOR/Home",
    "authors": [
        {
            "name": "Patrick Steele-Idem",
            "email": "psteeleidem@ebay.com"
        }
    ],
    
    "taglibs": {
	    "http://raptor.ebayopensource.org/core": "/templating/taglibs/core",
	    "http://raptor.ebayopensource.org/html": "/templating/taglibs/html",
	    "http://raptor.ebayopensource.org/widgets": "/templating/taglibs/widgets"
	},
	
	"include-handlers": {
	    "js": {"class": "packaging.include-handlers.IncludeHandler_js", "path": "/packaging/include-handlers/IncludeHandler_js.js"},
	    "css": {"class": "packaging.include-handlers.IncludeHandler_css", "path": "/packaging/include-handlers/IncludeHandler_css.js"},
	    "module": {"class": "packaging.include-handlers.IncludeHandler_module", "path": "/packaging/include-handlers/IncludeHandler_module.js"},
	    "rhtml": {"class": "packaging.include-handlers.IncludeHandler_rhtml", "path": "/packaging/include-handlers/IncludeHandler_rhtml.js"},
	    "rtld": {"class": "packaging.include-handlers.IncludeHandler_rtld", "path": "/packaging/include-handlers/IncludeHandler_rtld.js"} 
	}
}
