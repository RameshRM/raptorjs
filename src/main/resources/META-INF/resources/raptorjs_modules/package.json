{
    "type": "library",
    "name": "raptorjs",
    "version": "1.0",
    "description": "RaptorJS",
    "homepage": "http://wiki2.arch.ebay.com/display/RAPTOR/Home",
    "authors": [
        {
            "name": "Patrick Steele-Idem",
            "email": "psteeleidem@ebay.com"
        }
    ],
    
    "taglibs": {
	    "http://ebay.com/raptor/core": "/templating/taglibs/core",
	    "http://ebay.com/raptor/html": "/templating/taglibs/html",
	    "http://ebay.com/raptor/widgets": "/templating/taglibs/widgets"
	},
	
	"include-handlers": {
	    "js": {"class": "packaging.include-types.IncludeType_js", "path": "/packaging/include-types/IncludeType_js.js"},
	    "css": {"class": "packaging.include-types.IncludeType_css", "path": "/packaging/include-types/IncludeType_css.js"},
	    "module": {"class": "packaging.include-types.IncludeType_module", "path": "/packaging/include-types/IncludeType_module.js"},
	    "rhtml": {"class": "packaging.include-types.IncludeType_rhtml", "path": "/packaging/include-types/IncludeType_rhtml.js"},
	    "rtld": {"class": "packaging.include-types.IncludeType_rtld", "path": "/packaging/include-types/IncludeType_rtld.js"} 
	}
}
