<c:template 
    xmlns:c="http://ebay.com/raptor/core" 
    name="choose-when-attributes">

    <div id="one">
        <c:choose>
            <div c:when="false">A</div>
            <div c:when="false">B</div>
            <div c:otherwise="">TRUE</div>
        </c:choose>
    </div>,
    <div id="two">
        <c:choose>
            <div c:when="false">A</div>
            <div c:when="true">TRUE</div>
            <div c:otherwise="">C</div>
        </c:choose>
    </div>
    
</c:template>