<c:template 
    xmlns:c="core">
    
    <c:var name="colors" value="['red', 'green', 'blue']"/>
    
    <div c:for="color in colors">
        $color
    </div>
    
    <c:assign var="colors" value="['orange', 'purple', 'yellow']"/>
    
    <div c:for="color in colors">
        $color
    </div>
    
</c:template>