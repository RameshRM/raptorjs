<raptor-taglib>
    <tag>
        <handler-class>components.widgets.ButtonController</handler-class>
        <name>button</name>
        <attribute>
            <name>label</name>
        </attribute>
        <attribute>
            <name>id</name>
        </attribute>
        <attribute>
            <name>disabled</name>
            <type>boolean</type>
        </attribute>
        
        
        <!-- Support widget attributes -->
        <attribute>
            <uri>widgets</uri>
            <name>id</name>
        </attribute>
        
        
    </tag>
</raptor-taglib>